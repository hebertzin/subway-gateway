generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Roles {
  id String @unique @default(uuid())
  name String
  description String
  createAt DateTime @default(now())
}

model Manufacturer {
  id           String  @unique @default(uuid())
  name         String
  phone        String
  email        String
  street       String
  number       String
  city         String
  state        String
  zip_code     String
  website      String
}
